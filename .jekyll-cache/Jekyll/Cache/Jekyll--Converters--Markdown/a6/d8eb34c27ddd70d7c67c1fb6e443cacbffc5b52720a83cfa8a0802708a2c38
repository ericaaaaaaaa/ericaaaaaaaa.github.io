<h2 id="question">Question</h2>

<table>
  <thead>
    <tr>
      <th>Question</th>
      <th>Answer</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>$P\subseteq NP $</td>
      <td>yes</td>
    </tr>
    <tr>
      <td>$NP\subseteq P$</td>
      <td>unknown</td>
    </tr>
    <tr>
      <td>$NPC\subseteq NP $</td>
      <td>yes</td>
    </tr>
    <tr>
      <td>$P=NP $</td>
      <td>unknown</td>
    </tr>
  </tbody>
</table>

